{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: clang-10 build active file",
            "command": "/usr/bin/clang-10",
            "args": [
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "detail": "Task generated by Debugger."
        },
        {
            "label": "Build",
            "type": "shell",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "command": "make",
            "problemMatcher": []
        },
        {
            "label": "Cleanup",
            "type": "shell",
            "command": "rm speller",
            "problemMatcher": []
        }
    ],
    "version": "2.0.0"
}